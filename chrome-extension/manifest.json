{
  "manifest_version": 2,

  "name": "Cookie-Cleaner",
  "description": "Chrome extension to clean cookies from the current tab in one click.",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "remove cookies now"
  },
  "permissions": [
    "cookies", "tabs", "http://*/*", "https://*/*"
  ]
}
